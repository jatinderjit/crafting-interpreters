// Inheritance

class Base {
  init() {
    this.value = "base";
  }
  log() {
    print "From base:";
    print this.value;
  }
}
var base = Base();
base.log();

class Derived < Base {
   init() {
     this.value = "derived";
   }
   log2() {
      print "From Derived:";
      super.log();
   }
}
var derived = Derived();
derived.log();

print "************************************************";
class A {
  method() {
    print "A method";
  }
}

class B < A {}

class C < B {}

C().method();
