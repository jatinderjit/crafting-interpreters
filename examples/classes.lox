class Person {
  init(name) {
    this.name = name;
  }

  hi() {
    return "Hi";
  }

  greet() {
    print this.hi();
    print "I am";
    print this.name;
  }
}
var person = Person("J");
person.greet();
person.init("T");
person.greet();

